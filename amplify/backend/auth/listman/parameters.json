{
    "identityPoolName": "listman_idpool",
    "allowUnauthenticatedIdentities": false,
    "thirdPartyAuth": false,
    "lambdaLogPolicy": "listman_lambda_log_policy",
    "openIdLambdaRoleName": "listman_openid_lambda_role",
    "openIdRolePolicy": "listman_openid_pass_role_policy",
    "openIdLambdaIAMPolicy": "listman_openid_lambda_iam_policy",
    "openIdLogPolicy": "listman_openid_lambda_log_policy",
    "userPoolName": "listman_userpool",
    "autoVerifiedAttributes": [
        "email"
    ],
    "mfaConfiguration": "OFF",
    "mfaTypes": [
        "SMS Text Message"
    ],
    "roleName": "listman_sns-role",
    "roleExternalId": "listman_role_external_id",
    "policyName": "listman-sns-policy",
    "smsAuthenticationMessage": "Your authentication code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "Your verification code",
    "emailVerificationMessage": "Your verification code is {####}",
    "defaultPasswordPolicy": false,
    "passwordPolicyMinLength": 8,
    "passwordPolicyCharacters": [
        "Requires Lowercase",
        "Requires Uppercase",
        "Requires Numbers",
        "Requires Symbols"
    ],
    "requiredAttributes": [
        "email"
    ],
    "userpoolClientName": "listman_app_client",
    "userpoolClientGenerateSecret": true,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientReadAttributes": [
        "email"
    ],
    "mfaLambdaRole": "listman_totp_lambda_role",
    "mfaLambdaLogPolicy": "listman_totp_lambda_log_policy",
    "mfaPassRolePolicy": "listman_totp_pass_role_policy",
    "mfaLambdaIAMPolicy": "listman_totp_lambda_iam_policy",
    "userpoolClientLambdaRole": "listman_userpoolclient_lambda_role",
    "userpoolClientLogPolicy": "listman_userpoolclient_lambda_log_policy",
    "userpoolClientLambdaPolicy": "listman_userpoolclient_lambda_iam_policy",
    "userpoolClientSetAttributes": true,
    "useDefault": "manual",
    "authSelections": "identityPoolAndUserPool",
    "resourceName": "listman",
    "userpoolClientWriteAttributes": [
        "email"
    ],
    "authRoleName": {
        "Ref": "AuthRoleName"
    },
    "unauthRoleName": {
        "Ref": "UnauthRoleName"
    },
    "authRoleArn": {
        "Fn::GetAtt": [
            "AuthRole",
            "Arn"
        ]
    },
    "unauthRoleArn": {
        "Fn::GetAtt": [
            "UnauthRole",
            "Arn"
        ]
    }
}
{
  "name": "vue-ssr-serverless",
  "version": "1.0.0",
  "description": "Vue SSR Serverless Minimum Example",
  "main": "handler.js",
  "dependencies": {
    "aws-amplify": "^1.1.26",
    "aws-sdk": "^2.449.0",
    "cookie": "^0.3.1",
    "esm": "^3.2.5",
    "googleapis": "^36.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "mongodb": "^3.2.3",
    "node-fetch": "^2.5.0",
    "vue": "^2.6.6",
    "vue-router": "^3.0.6",
    "vue-server-renderer": "^2.6.6",
    "vuex": "^3.1.0",
    "vuex-router-sync": "^5.0.0",
    "ws": "^6.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "bulma": "^0.7.4",
    "css-loader": "^1.0.1",
    "hapi-plugin-websocket": "^1.2.19",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "serverless-domain-manager": "^2.6.13",
    "serverless-finch": "^2.4.2",
    "serverless-offline": "github:y15e/serverless-offline#websocket",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.6",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "deploy": "npm run build && npm run deploy:client && serverless deploy",
    "deploy:prod": "npm run build && npm run deploy:client && serverless deploy --stage prod",
    "deploy:client": "serverless client deploy --no-confirm",
    "deploy:func": "npm run build && npm run deploy:client && serverless deploy function -f index",
    "build": "npm run build:server && npm run build:client",
    "build:client": "npx webpack --config build/webpack.client.config.js",
    "build:server": "npx webpack --config build/webpack.server.config.js",
    "build:css": "node-sass --omit-source-map-url sass/bulma.scss dist/bulma.css",
    "offline": "serverless offline start --dontPrintOutput --skipCacheInvalidation --stage prod",
    "logs": "serverless logs -f websocket -t"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/y15e/listman.git"
  },

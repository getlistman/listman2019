{
  "name": "vue-ssr-serverless",
  "version": "1.0.0",
  "description": "Vue SSR Serverless Minimum Example",
  "main": "handler.js",
  "dependencies": {
    "aws-amplify": "^1.1.18",
    "aws-sdk": "^2.384.0",
    "cookie": "^0.3.1",
    "esm": "^3.0.84",
    "googleapis": "^36.0.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "mongodb": "^3.1.10",
    "node-fetch": "^2.3.0",
    "serverless-websockets-plugin": "^1.0.0",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.5.21",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "bulma": "^0.7.2",
    "css-loader": "^1.0.1",
    "hapi-plugin-websocket": "^1.2.19",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "serverless-domain-manager": "^2.6.10",
    "serverless-finch": "^2.3.2",
    "serverless-offline": "^3.32.1",
    "vue-loader": "^15.5.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.21",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "deploy": "npm run build && npm run deploy:client && serverless deploy",
    "deploy:client": "serverless client deploy --no-confirm",
    "deploy:func": "npm run build && npm run deploy:client && serverless deploy function -f index",
    "build": "npm run build:server && npm run build:client",
    "build:client": "npx webpack --config build/webpack.client.config.js",
    "build:server": "npx webpack --config build/webpack.server.config.js",
    "build:css": "node-sass --omit-source-map-url sass/bulma.scss dist/bulma.css",
    "offline": "serverless offline start --dontPrintOutput --skipCacheInvalidation",
    "logs": "serverless logs -f websocket -t"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d6er/vue-ssr-serverless.git"
  },
